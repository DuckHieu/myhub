local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


--Values
_G.autoclick = true
_G.autoegg = "Basic Egg"


---func
function autoclick()
    while _G.autoclick == true do
        game:GetService("ReplicatedStorage").Packages.Knit.Services.ClickService.RF.Click:InvokeServer()
        wait(0.000000000000000000000000000000000000000000000001)
     end   
end
function autoegg()
    while _G.autoegg == true do
        game:GetService("ReplicatedStorage").Packages.Knit.Services.EggService.RF.OpenEgg:InvokeServer("Basic Egg","Single")
        wait(0.001)
     end  
end







----tabs
local FTab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FTab:AddToggle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		_G.autoclick = Value
        autoclick()
	end    
})
FTab:AddDropdown({
	Name = "Select Egg",
	Default = "Basic",
	Options = {"Basic", "Rare"},
	Callback = function(Value)
		_G.autoegg = Value
        autoegg()
	end    
})

